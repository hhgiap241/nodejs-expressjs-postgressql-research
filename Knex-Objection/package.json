    "knex": "^2.3.0",
    "morgan": "^1.10.0",
    "objection": "^3.0.1",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.11",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
